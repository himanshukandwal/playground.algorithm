package dev.research.himanshu.algorithm.dynamicprogramming;

import junit.framework.TestCase;

public class MaxValueContiguousSubsequenceTest extends TestCase {

	private MaxValueContiguousSubsequence maxValueContiguousSubsequence;
	
	@Override
	protected void setUp() throws Exception {
		maxValueContiguousSubsequence = new MaxValueContiguousSubsequence();
		super.setUp();
	}
	
	public MaxValueContiguousSubsequence getMaxValueContiguousSubsequence() {
		return maxValueContiguousSubsequence;
	}
	
	public void testGetMaxSequenceSubsequenceForNoInput() {
		assertNull(getMaxValueContiguousSubsequence().getMaxSequenceSubsequence());
	}
	
	public void testGetMaxSequenceSubsequenceForPositiveInput() {
		getMaxValueContiguousSubsequence()
		assertNull(getMaxValueContiguousSubsequence().getMaxSequenceSubsequence());
	}

}
